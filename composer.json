{
  "name": "tourze/alipay-mini-program-bundle",
  "description": "支付宝小程序模块",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "alipaysdk/openapi": "^2.5",
    "tourze/doctrine-indexed-bundle": "0.0.*",
    "tourze/easy-admin-menu-bundle": "0.1.*",
    "tourze/easy-admin-attribute": "0.1.*",
    "symfony/http-kernel": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/config": "^6.4",
    "doctrine/orm": "^3.0",
    "doctrine/dbal": "^4.0",
    "doctrine/doctrine-bundle": "^2.13",
    "psr/log": "^3|^2|^1"
  },
  "autoload": {
    "psr-4": {
      "AlipayMiniProgramBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AlipayMiniProgramBundle\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^2.1"
  }
}