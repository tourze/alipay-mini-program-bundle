{
  "name": "tourze/alipay-mini-program-bundle",
  "description": "支付宝小程序模块",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "alipaysdk/openapi": "^2.5",
    "doctrine/collections": "^2.3",
    "doctrine/dbal": "^4.0",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^3.0",
    "doctrine/persistence": "^3.1 || ^4",
    "lcobucci/jwt": "^5.0",
    "nesbot/carbon": "^2.72 || ^3",
    "psr/log": "^3|^2|^1",
    "symfony/config": "^6.4",
    "symfony/console": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/http-foundation": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/security-bundle": "^6.4",
    "symfony/security-core": "^6.4",
    "tourze/bundle-dependency": "0.0.*",
    "tourze/doctrine-indexed-bundle": "0.0.*",
    "tourze/doctrine-timestamp-bundle": "0.0.*",
    "tourze/easy-admin-attribute": "0.1.*",
    "tourze/json-rpc-core": "^0.0.6",
    "tourze/json-rpc-lock-bundle": "0.1.*",
    "tourze/symfony-cron-job-bundle": "0.1.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "AlipayMiniProgramBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AlipayMiniProgramBundle\\Tests\\": "tests/"
    }
  }
}